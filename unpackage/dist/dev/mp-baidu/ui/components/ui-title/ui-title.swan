<view class="{{['ui-title',ui,align,tpl,bg,[(inLine)?'line':''],'heading-'+depth]}}"><view class="anchor" style="{{'top:'+('-'+sys_navBar+'px')+';'}}" id="{{title}}"></view><view class="{{['action',hasLine?'hasLine':'',hasDot?'hasDot':'']}}"><view class="action-icon _div"><block s-if="{{isIcon}}"><block><block s-if="{{depth<3}}"><view class="ui-TC-Main _icon-titles"></view></block><block s-if="{{depth==3}}"><view class="ui-TC-Main _icon-title"></view></block><block s-if="{{depth>3}}"><view class="ui-TC-Main _icon-title icon-sm"></view></block></block></block><slot name="icon"></slot></view><block s-if="{{title!=''}}"><view class="{{['action-title',titleUi]}}">{{''+title+''}}<slot></slot><block s-if="{{hasLine}}"><view class="{{['action-line',line]}}"><view></view><view></view></view></block><block s-if="{{hasDot}}"><view class="{{['action-dot',dot]}}"><view></view><view></view></view></block></view></block><block s-if="{{desc!=''}}"><view class="{{['action-desc',descUi]}}">{{desc}}</view></block></view><view class="more"><slot name="more"></slot></view></view>