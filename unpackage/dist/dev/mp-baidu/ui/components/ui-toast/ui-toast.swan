<view class="{{['ui-toast-box',title!=''?'show':'hide',[(mask||mask=='80')?'bg-mask-80':''],[(mask=='20')?'bg-mask-20':''],[(mask=='40')?'bg-mask-40':'']]}}" style="{{'top:'+(sys_navBar+'px')+';'}}"><view class="{{['ui-toast','bg-mask-80',[(icon!='')?'hasIcon':'']]}}"><block s-if="{{icon!=''}}"><view class="ui-toast-icon"><text class="{{[icon,[(isLoading)?'icon-spin':'']]}}"></text></view></block><text>{{title.length>255?$root.g0+'...':title}}</text></view></view>