<view data-event-opts="{{[['tap',[['_onRadioClick',['$event']]]]]}}" class="{{['ui-radio',[(isDisabled)?'disabled':''],[(src)?'img':''],ui]}}" bindtap="__e"><block s-if="{{!none}}"><view class="{{['ui-radio-input',isChecked?'cur '+bg:unbg,src?'radius':'round']}}"></view></block><block s-if="{{src}}"><image class="ui-radio-img radius" src="{{src}}" mode="aspectFill"></image></block><block s-else><view class="ui-radio-content"><slot></slot></view></block><block s-if="{{$root.m0}}"><view class="{{['ui-radio-bg','round',isChecked?'cur '+bg:'']}}"></view></block></view>